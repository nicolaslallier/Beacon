  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "$prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "red",
                  "index": 1,
                  "text": "DOWN"
                },
                "1": {
                  "color": "green",
                  "index": 0,
                  "text": "UP"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "10.4.2",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "up{job=\"beacon-vectordb-postgres\"}",
          "refId": "A"
        }
      ],
      "title": "Beacon-VectorDB Exporter",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "$prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "id": 2,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "pg_stat_database_numbackends{datname=\"$db\"}",
          "refId": "A"
        }
      ],
      "title": "Active Connections",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "$prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "rate(pg_stat_database_xact_commit{datname=\"$db\"}[$__rate_interval])",
          "legendFormat": "commits/s",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "rate(pg_stat_database_xact_rollback{datname=\"$db\"}[$__rate_interval])",
          "legendFormat": "rollbacks/s",
          "refId": "B"
        }
      ],
      "title": "Transactions Per Second",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "$prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "id": 4,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "pg_stat_database_blks_hit{datname=\"$db\"} / (pg_stat_database_blks_hit{datname=\"$db\"} + pg_stat_database_blks_read{datname=\"$db\"})",
          "legendFormat": "cache_hit_ratio",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Ratio",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "$prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$prometheus"
          },
          "expr": "pg_database_size_bytes{datname=\"$db\"}",
          "legendFormat": "db_size_bytes",
          "refId": "A"
        }
      ],
      "title": "Database Size",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "$loki"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "id": 6,
      "options": {
        "reduceOptions": {
          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "$loki"
          },
          "expr": "sum(count_over_time({container=\"$container\"}[$__range]))",
          "refId": "A"
        }
      ],
      "title": "Log Lines",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "$loki"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "id": 7,
      "options": {
        "reduceOptions": {
          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "$loki"
          },
          "expr": "sum(count_over_time({container=\"$container\"} |~ \"(?i)error|fail|fatal|exception\" [$__range]))",
          "refId": "A"
        }
      ],
      "title": "Error Log Lines",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "$loki"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 20
      },
      "id": 8,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "$loki"
          },
          "expr": "{container=\"$container\"}",
          "refId": "A"
        }
      ],
      "title": "Recent Logs",
      "type": "logs"
    }
